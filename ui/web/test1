#this test script is for authentication*******************
#!/bin/csh
echo "\n"
echo "____________________USER AUTHENTICATION_______________________\n"

setenv REQUEST_METHOD GET
setenv QUERY_STRING username=user\&password=user1
login.cgi |  grep "user" | cut -c17-33 | tr ";" " " > login.tmp  
nl  login.tmp

echo "________________________________________________________________"
setenv REQUEST_METHOD GET
setenv QUERY_STRING username=user\&password=user2
login.cgi |  grep "user"| cut -c17-33 | tr ";" " " > login.tmp
nl login.tmp
echo "________________________________________________________________"
setenv REQUEST_METHOD GET
setenv QUERY_STRING username=user\&password=user3
login.cgi | grep "user"| cut -c17-33 | tr ";" " " > login.tmp
nl login.tmp
echo "________________________________________________________________"
setenv REQUEST_METHOD GET
setenv QUERY_STRING username=nobody\&password=user
login.cgi | grep "user"  > login.tmp
nl login.tmp
echo "________________________________________________________________"
